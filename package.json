    "commander": "2.6.0",
    "mkdirp": "0.5.0",
    "request": "2.53.0",
    "xml2js": "0.4.5"
  },
  "devDependencies": {
    "tsd": "0.5.7",
    "tslint": "2.3.0-beta",
    "typescript": "1.5.0-beta"
