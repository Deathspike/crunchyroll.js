  "keywords": [
    "anime",
    "download",
    "crunchyroll"
  ],
  "name": "crunchy",
  "repository": {
    "type": "git",
    "url": "git://github.com/Godzil/crunchyroll.js.git"
  },
  "version": "1.1.7",
  "bin": {
    "crunchy": "./bin/crunchy"
  },
  "dependencies": {
    "big-integer": "1.4.4",
